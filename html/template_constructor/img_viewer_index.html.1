function Sure(dir,file,sid) {
  if (confirm("Вы уверены, что хотите удалить файл/директорию?\n"+' '+dir+''+file)) {
        return true;
  }
  else {return false;}
}

var myPage = window.opener;
function addLinkImg()
{  var image;
    image=document.imageLinkForm.link.value;
    myPage.selectImage(image);
    self.close();
} 

function SelectImage(ImageName) {
        window.opener.selectImage("$IMG_PATH" + ImageName);
        self.close();
}

function SelectFile(FileName,size,ext) {
        window.opener.selectFile("$IMG_PATH" + FileName,size,ext);
        self.close();
}
function OpenFile( fileUrl )
{
 window.top.opener.SetUrl( "$IMG_PATH" + encodeURI( fileUrl ).replace( '#', '%23' ) ) ;
 window.top.close() ;
 window.top.opener.focus() ;
}
this.focus();

</script>
